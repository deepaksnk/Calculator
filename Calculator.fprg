                <output expression="&quot;&#13;&#10;1(Addition) &#13;&#10;2(Subraction) &#13;&#10;3(Multiplication) &#13;&#10;4(Division) &#13;&#10;Enter the choice &#13;&#10;&quot;" newline="True"/>
                <input variable="n"/>
