                            </then>
                            <else>
                                <if expression="n=3">
                                    <then>
                                        <output expression="&quot;&#13;&#10;(a+ib) &#13;&#10;Enter value a &#13;&#10;&quot;" newline="True"/>
                                        <input variable="a"/>
                                        <output expression="&quot;&#13;&#10;(a+ib) &#13;&#10;Enter value b &#13;&#10;&quot;" newline="True"/>
                                        <input variable="b"/>
                                        <output expression="&quot;&#13;&#10;(c+id) &#13;&#10;Enter value c &#13;&#10;&quot;" newline="True"/>
