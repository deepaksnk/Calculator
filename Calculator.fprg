    <function name="vecan" type="None" variable="">
        <parameters/>
        <body>
            <declare name="a, b, a1, a2, a3, b1, b2, b3, dotproduct, deta, detb, n, rad, deg" type="Real" array="False" size=""/>
            <output expression="&quot;Enter the ith component(a1) of the vector a = (a1)i + (b1)j + (c1)k and b = (a2)i + (b2)j + (c2)k &quot;" newline="True"/>
            <input variable="a1"/>
            <output expression="&quot;Enter the jth component(b1) of the vector a = (a1)i + (b1)j + (c1)k and b = (a2)i + (b2)j + (c2)k &quot;" newline="True"/>
            <input variable="a2"/>
            <output expression="&quot;Enter the kth component(c1) of the vector a = (a1)i + (b1)j + (c1)k and b = (a2)i + (b2)j + (c2)k &quot;" newline="True"/>
            <input variable="a3"/>
            <output expression="&quot;Enter the ith component(a2) of the vector a = (a1)i + (b1)j + (c1)k and b = (a2)i + (b2)j + (c2)k &quot;" newline="True"/>
            <input variable="b1"/>
            <output expression="&quot;Enter the jth component(b2) of the vector a = (a1)i + (b1)j + (c1)k and b = (a2)i + (b2)j + (c2)k &quot;" newline="True"/>
            <input variable="b2"/>
            <output expression="&quot;Enter the kth component(c2) of the vector a = (a1)i + (b1)j + (c1)k and b = (a2)i + (b2)j + (c2)k &quot;" newline="True"/>
            <input variable="b3"/>
            <assign variable="dotproduct" expression="(a1*b1)+(a2*b2)+(a3*b3)"/>
            <assign variable="deta" expression="sqrt(a1^2+a2^2+a3^2)"/>
            <assign variable="detb" expression="sqrt(b1^2+b2^2+b3^2)"/>
            <assign variable="n" expression="dotproduct/(deta*detb)"/>
            <assign variable="rad" expression="arccos(n)"/>
            <assign variable="deg" expression="(rad*180)/3.14159"/>
            <output expression="&quot;The angle between the two vectors a= (&quot; &amp; a1&amp; &quot;)i+ (&quot; &amp;a2&amp; &quot; )j+(&quot; &amp;a3&amp; &quot;)k and b=(&quot;&amp;b1&amp; &quot;)i+(&quot; &amp;b2&amp; &quot;)j+(&quot; &amp;b3&amp; &quot;)k is displayed below&quot;" newline="True"/>
            <output expression="&quot;&quot; &amp;deg&amp; &quot; degrees &quot;" newline="True"/>
        </body>
    </function>
