                    <output expression="&quot;The addition of two vectors of a = (&quot; &amp; a1&amp; &quot;)i + (&quot; &amp;b1&amp; &quot;)j and b = (&quot;&amp;a2&amp; &quot;)i + (&quot; &amp;b2&amp; &quot;)j &quot;" newline="True"/>
                    <output expression="&quot;(&quot; &amp; twodimendot &amp; &quot;)i + (&quot; &amp; twodimendot2 &amp; &quot;)j&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="dimension=3">
                        <then>
