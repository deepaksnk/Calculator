    <function name="frac" type="None" variable="">
        <parameters/>
        <body>
            <declare name="a, b, c, d, sn" type="Real" array="False" size=""/>
            <declare name="n, stop" type="Integer" array="False" size=""/>
            <assign variable="stop" expression="1"/>
            <while expression="stop!=0">
                <output expression="&quot;&#13;&#10;1(Addition with same denominator) &#13;&#10;2(Addition with different denominator) &#13;&#10;3(Subraction with same denominator) &#13;&#10;4(Subraction with different denominator) &#13;&#10;5(Multiplication) &#13;&#10;6(Division) &#13;&#10;Enter the choice &#13;&#10;&quot;" newline="True"/>
                <input variable="n"/>
                <if expression="n=1">
                    <then>
                        <output expression="&quot;Enter numerator 1&quot;" newline="True"/>
                        <input variable="a"/>
                        <output expression="&quot;Enter numerator 2&quot;" newline="True"/>
                        <input variable="c"/>
                        <output expression="&quot;Enter denominator&quot;" newline="True"/>
                        <input variable="b"/>
                        <assign variable="sn" expression="(a/b) + (c/b)"/>
                        <output expression="sn" newline="True"/>
                    </then>
                    <else>
                        <if expression="n=2">
                            <then>
                                <output expression="&quot;Enter numerator 1&quot;" newline="True"/>
                                <input variable="a"/>
                                <output expression="&quot;Enter denominator 1&quot;" newline="True"/>
                                <input variable="b"/>
                                <output expression="&quot;Enter numerator 2&quot;" newline="True"/>
                                <input variable="c"/>
                                <output expression="&quot;Enter denominator 2&quot;" newline="True"/>
                                <input variable="d"/>
                                <assign variable="sn" expression="((a*d)+(b*c))/(b*d)"/>
                                <output expression="sn" newline="True"/>
                            </then>
                            <else>
                                <if expression="n=3">
                                    <then>
                                        <output expression="&quot;Enter numerator 1&quot;" newline="True"/>
                                        <input variable="a"/>
                                        <output expression="&quot;Enter numerator 2&quot;" newline="True"/>
                                        <input variable="c"/>
                                        <output expression="&quot;Enter denominator&quot;" newline="True"/>
                                        <input variable="b"/>
                                        <assign variable="sn" expression="(a/b) - (c/b)"/>
                                        <output expression="sn" newline="True"/>
                                    </then>
                                    <else>
                                        <if expression="n=4">
                                            <then>
                                                <output expression="&quot;Enter numerator 1&quot;" newline="True"/>
                                                <input variable="a"/>
                                                <output expression="&quot;Enter denominator 1&quot;" newline="True"/>
                                                <input variable="b"/>
                                                <output expression="&quot;Enter numerator 2&quot;" newline="True"/>
                                                <input variable="c"/>
                                                <output expression="&quot;Enter denominator 2&quot;" newline="True"/>
                                                <input variable="d"/>
                                                <assign variable="sn" expression="((a*d)-(b*c))/(b*d)"/>
                                                <output expression="sn" newline="True"/>
                                            </then>
                                            <else>
                                                <if expression="n=5">
                                                    <then>
                                                        <output expression="&quot;Enter numerator 1&quot;" newline="True"/>
                                                        <input variable="a"/>
                                                        <output expression="&quot;Enter denominator 1&quot;" newline="True"/>
                                                        <input variable="b"/>
                                                        <output expression="&quot;Enter numerator 2&quot;" newline="True"/>
                                                        <input variable="c"/>
                                                        <output expression="&quot;Enter denominator 2&quot;" newline="True"/>
                                                        <input variable="d"/>
                                                        <assign variable="sn" expression="(a*c)/(b*d)"/>
                                                        <output expression="sn" newline="True"/>
                                                    </then>
                                                    <else>
                                                        <if expression="n=6">
                                                            <then>
                                                                <output expression="&quot;Enter numerator 1&quot;" newline="True"/>
                                                                <input variable="a"/>
                                                                <output expression="&quot;Enter denominator 1&quot;" newline="True"/>
                                                                <input variable="b"/>
                                                                <output expression="&quot;Enter numerator 2&quot;" newline="True"/>
                                                                <input variable="c"/>
                                                                <output expression="&quot;Enter denominator 2&quot;" newline="True"/>
                                                                <input variable="d"/>
                                                                <assign variable="sn" expression="(a*d)/(b*c)"/>
                                                                <output expression="sn" newline="True"/>
                                                            </then>
                                                            <else>
                                                                <output expression="&quot;Incorrect Choice&quot;" newline="True"/>
                                                            </else>
                                                        </if>
                                                    </else>
                                                </if>
                                            </else>
                                        </if>
                                    </else>
                                </if>
                            </else>
                        </if>
                    </else>
