    <function name="basic" type="None" variable="">
        <parameters/>
        <body>
            <declare name="n, number, index, operation, sum, difference, product, division, stop" type="Real" array="False" size=""/>
            <assign variable="stop" expression="1"/>
            <while expression="stop!=0">
                <output expression="&quot;Enter how many values to be Calculated&quot;" newline="True"/>
                <input variable="n"/>
                <declare name="num" type="Real" array="True" size="n"/>
                <declare name="div" type="Real" array="True" size="n"/>
                <output expression="&quot;&#13;&#10;1(Sum)&#13;&#10;2(Difference)&#13;&#10;3(Product)&#13;&#10;4(Quotient)&#13;&#10;5(Remainder)&#13;&#10;&#13;&#10;Enter the choice &#13;&#10;&quot;" newline="True"/>
                <input variable="operation"/>
                <if expression="operation &gt;0 &amp;&amp; operation&lt;=5">
                    <then>
                        <if expression="operation=1">
                            <then>
                                <output expression="&quot;Enter the Values&quot;" newline="True"/>
                                <for variable="index" start="0" end="n-1" direction="inc" step="1">
                                    <output expression="&quot;Enter element no :&quot; &amp; (index+1)" newline="True"/>
                                    <input variable="number"/>
                                    <assign variable="num[index]" expression="number"/>
                                </for>
                                <assign variable="sum" expression="0"/>
                                <for variable="index" start="0" end="n-1" direction="inc" step="1">
                                    <assign variable="sum" expression="sum+num[index]"/>
                                </for>
                                <output expression="&quot;The sum of the given numbers is &quot; &amp; sum" newline="True"/>
                            </then>
                            <else>
                                <if expression="operation=2">
                                    <then>
                                        <declare name="t" type="Integer" array="False" size=""/>
                                        <output expression="&quot;Enter the Values&quot;" newline="True"/>
                                        <output expression="&quot;Enter element no :&quot; &amp; &quot;1&quot;" newline="True"/>
                                        <input variable="number"/>
                                        <assign variable="t" expression="number"/>
                                        <for variable="index" start="0" end="n-2" direction="inc" step="1">
                                            <output expression="&quot;Enter element no :&quot; &amp; (index+2)" newline="True"/>
                                            <input variable="number"/>
                                            <assign variable="num[index]" expression="number"/>
                                        </for>
                                        <assign variable="sum" expression="0"/>
                                        <for variable="index" start="0" end="n-2" direction="inc" step="1">
                                            <assign variable="sum" expression="sum+num[index]"/>
                                        </for>
                                        <assign variable="difference" expression="t-sum"/>
                                        <output expression="&quot;The difference between the given numbers is &quot; &amp; difference" newline="True"/>
                                    </then>
                                    <else>
                                        <if expression="operation=3">
                                            <then>
                                                <output expression="&quot;Enter the Values&quot;" newline="True"/>
                                                <for variable="index" start="0" end="n-1" direction="inc" step="1">
                                                    <output expression="&quot;Enter element no :&quot; &amp; (index+1)" newline="True"/>
                                                    <input variable="number"/>
                                                    <assign variable="num[index]" expression="number"/>
                                                </for>
                                                <assign variable="product" expression="1"/>
                                                <for variable="index" start="0" end="n-1" direction="inc" step="1">
                                                    <assign variable="product" expression="product*num[index]"/>
                                                </for>
                                                <output expression="&quot;The product of the given numbers is &quot; &amp; product" newline="True"/>
                                            </then>
                                            <else>
                                                <if expression="operation=4">
                                                    <then>
                                                        <output expression="&quot;Enter the Values&quot;" newline="True"/>
                                                        <for variable="index" start="0" end="n-1" direction="inc" step="1">
                                                            <output expression="&quot;Enter element no :&quot; &amp; (index+1)" newline="True"/>
                                                            <input variable="number"/>
                                                            <assign variable="div[index]" expression="number"/>
                                                        </for>
                                                        <declare name="div2" type="Real" array="True" size="1"/>
                                                        <assign variable="div2[0]" expression="0"/>
                                                        <for variable="index" start="0" end="n-1" direction="inc" step="1">
                                                            <if expression="index!=0">
                                                                <then>
                                                                    <assign variable="div2[0]" expression="div2[0]/div[index]"/>
                                                                </then>
                                                                <else>
                                                                    <assign variable="div2[0]" expression="div[index]"/>
                                                                </else>
                                                            </if>
                                                        </for>
                                                        <output expression="&quot;The quotient of the given numbers is &quot; &amp;div2[0]" newline="True"/>
                                                    </then>
                                                    <else>
                                                        <if expression="operation=5">
                                                            <then>
                                                                <output expression="&quot;Enter the Values&quot;" newline="True"/>
                                                                <for variable="index" start="0" end="n-1" direction="inc" step="1">
                                                                    <output expression="&quot;Enter element no :&quot; &amp; (index+1)" newline="True"/>
                                                                    <input variable="number"/>
                                                                    <assign variable="div[index]" expression="number"/>
                                                                </for>
                                                                <declare name="div2" type="Real" array="True" size="1"/>
                                                                <assign variable="div2[0]" expression="0"/>
                                                                <for variable="index" start="0" end="n-1" direction="inc" step="1">
                                                                    <if expression="index!=0">
                                                                        <then>
                                                                            <assign variable="div2[0]" expression="div2[0]%div[index]"/>
                                                                        </then>
                                                                        <else>
                                                                            <assign variable="div2[0]" expression="div[index]"/>
                                                                        </else>
                                                                    </if>
                                                                </for>
                                                                <output expression="&quot;the remainder of the given numbers is&quot; &amp;div2[0]" newline="True"/>
                                                            </then>
                                                            <else/>
                                                        </if>
                                                    </else>
                                                </if>
                                            </else>
                                        </if>
                                    </else>
                                </if>
                            </else>
                        </if>
                    </then>
                    <else>
                        <output expression="&quot;Incorrect Choice&quot;" newline="True"/>
                    </else>
                </if>
                <output expression="&quot;Enter 0 to Exit or&#13;&#10;Press any other  number for Menu&quot;" newline="True"/>
                <input variable="stop"/>
            </while>
        </body>
    </function>
    <function name="comp" type="None" variable="">
        <parameters/>
        <body>
            <declare name="a, b, c, d, x, y, k, sn" type="Integer" array="False" size=""/>
            <declare name="n, stop" type="Integer" array="False" size=""/>
