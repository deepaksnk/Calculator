                                <input variable="b"/>
                                <output expression="&quot;&#13;&#10;(c+id) &#13;&#10;Enter value c &#13;&#10;&quot;" newline="True"/>
                                <input variable="c"/>
                                <output expression="&quot;&#13;&#10;(c+id) &#13;&#10;Enter value d &#13;&#10;&quot;" newline="True"/>
                                <input variable="d"/>
                                <assign variable="x" expression="(a-c)"/>
                                <assign variable="y" expression="(b-d)"/>
